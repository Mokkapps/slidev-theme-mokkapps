{
  "name": "slidev-theme-mokkapps",
  "version": "1.1.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png",
    "screenshot:dark": "slidev export example.md --dark --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.38.3"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.1.22",
    "@iconify-json/uim": "^1.1.3",
    "@slidev/cli": "^0.38.3",
    "playwright-chromium": "^1.29.2"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "defaults": {
      "favicon": "https://mokkapps.de/favicon.ico",
      "fonts": {
        "sans": "Open Sans",
        "title": "Domine",
        "mono": "Cascadia Code"
      }
    }
  }
}
